### subscription-url = 替换这段中文为你的机场订阅链接
# 将proxy-providers里面链接中的“替换这段中文为你的机场订阅链接”全部替换即可
# 第一行为了兼容Choc的机场流量显示
mixed-port: 7890
allow-lan: true
bind-address: '*'
mode: rule
log-level: info
ipv6: true
external-controller: 127.0.0.1:9090


proxy-providers:

  #全部节点
  All: &All
    type: http
    url: "https://api.v1.mk/sub?target=clash&url=替换这段中文为你的机场订阅链接&emoji=true&list=true&udp=true"
    interval: 3600
    path: ./Proxy/ALL.yaml
    health-check:
      enable: true
      interval: 600
      url: http://www.gstatic.com/generate_204

  #美国节点    
  US:
    <<: *All #引用锚点内容
    filter: 美|🇺🇲|US|(?i)States|American

  #香港节点
  HK:
    <<: *All
    filter: 港|🇭🇰|HK|(?i)Hong

  #日本节点
  JP:
    <<: *All
    filter: 日|🇯🇵|东京|JP|(?i)Japan

  #狮城节点
  SG:
    <<: *All
    filter: 新|🇸🇬|狮城|SG|(?i)Singapore

 
  #台湾节点
  TW:
    <<: *All
    filter: 台|🇨🇳|🇹🇼|湾|TW|(?i)Taiwan


proxies:

            

